// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model RocketPool {
  block_number    BigInt
  block_timestamp BigInt
  log_index       BigInt

  eth String
  rpl String

  @@unique([block_number, log_index])
}

model EtherFi {
  block_number    BigInt
  block_timestamp BigInt
  log_index       BigInt

  eth String

  @@unique([block_number, log_index])
}

model StakeWise {
  block_number    BigInt
  block_timestamp BigInt
  log_index       BigInt

  vault String
  eth   String

  @@unique([block_number, log_index])
}
